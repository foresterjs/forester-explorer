{
  "name": "forester-explorer",
  "version": "0.1.2",
  "description": "Explorer for Forester",
  "main": "build/index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run-script clean && npm run-script build",
    "clean": "rm -rf ./build && mkdir ./build",
    "build": "babel -d ./build ./lib -s",
    "example": "babel-node example/example.js",
    "postinstall": "bower --allow-root install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foresterjs/forester-explorer.git"
  },
  "keywords": [
    "rest",
    "api"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/foresterjs/forester-explorer/issues"
  },
  "homepage": "https://github.com/foresterjs/forester-explorer#readme",
  "devDependencies": {
    "babel": "^6.2.0",
    "babel-cli": "^6.2.0",
    "nodemon": "^1.8.0",
    "babel-plugin-transform-async-to-generator": "^6.0.14",
    "babel-plugin-transform-es2015-arrow-functions": "^6.0.14",
    "babel-plugin-transform-es2015-classes": "^6.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.0.18",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.1.3",
    "babel-plugin-transform-es2015-parameters": "^6.0.18",
    "babel-plugin-transform-es2015-spread": "^6.0.14"
  },
  "dependencies": {
    "babel-plugin-transform-async-functions": "^6.1.18",
    "bower": "^1.6.5",
    "forester": "^0.3.3"
  },
  "babel": {
    "plugins": [
      "transform-es2015-arrow-functions",
      "transform-es2015-classes",
      "transform-es2015-destructuring",
      "transform-async-to-generator",
      "transform-es2015-parameters",
      "transform-es2015-modules-commonjs",
      "transform-es2015-spread"
    ],
    "ignore": false
  }
}
